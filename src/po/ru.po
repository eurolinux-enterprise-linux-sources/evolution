msgid "Error saving {0} to {1}: {2}"
msgstr "Ошибка сохранения {0} в {1}: {2}"

#: ../addressbook/addressbook.error.xml.h:15
msgid "Failed to authenticate with LDAP server."
msgstr "Сбой проверки подлинности на сервере LDAP."

